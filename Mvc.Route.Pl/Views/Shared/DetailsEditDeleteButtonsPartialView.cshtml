@model string

<td>
    <a class="btn btnStyl btn-outline-info" asp-action="Details" asp-route-id="@Model">
        <i class="fa-solid fa-eye"></i>

    </a>
</td>
<td>
    <a class="btn btnStyl btn-outline-warning" asp-action="Update" asp-route-id="@Model">
        <i class="fa-solid fa-edit"></i>

    </a>
</td>
<td>
    <a class="btn btnStyl btn-outline-danger" asp-action="Delete" asp-route-id="@Model">
        <i class="fa-solid fa-trash"></i>

    </a>
</td>
