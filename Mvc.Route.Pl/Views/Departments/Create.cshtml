@model DepartmentViewModel
@{
	ViewData["Title"] = "Create";
}

<h1 >Create New Department</h1>
<br />
<br />
<form asp-action="Create">
	<partial name="DepartmentPartialViews/DepartmentEditAddPartialView" model="@Model"></partial>

	<input class="btn btnStyl btn-outline-success" type="submit" />
	<a asp-action="Index" class="btn btnStyl btn-outline-dark">Back to List</a>

</form>



@section jQueryValidation{
	<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
	<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}